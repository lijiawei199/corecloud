        type: string
        description: 换班时间

  FlowViewVO:
    type: object
    description: 流量总览上半区
    properties:
      totalflow:
        type: integer
        description: 今日总客流量
      yestflow:
        type: integer
        description: 昨日总客流量
      usedstore:
        type: integer
        description: 已入驻商户数量
      totalstore:
        type: integer
        description: 总商户数量  
