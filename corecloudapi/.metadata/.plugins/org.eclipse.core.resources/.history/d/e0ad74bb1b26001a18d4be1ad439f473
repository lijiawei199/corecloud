        type: integer
        description: 使用面积
      usedarea:
        type: integer
        description: 总面积
      power:
        type: integer
        description: 今日集客力
      yestpower:
        type: integer
        description: 昨日集客力
  FlowTrendVO:
    type: object
    description: 流量趋势
    properties:
