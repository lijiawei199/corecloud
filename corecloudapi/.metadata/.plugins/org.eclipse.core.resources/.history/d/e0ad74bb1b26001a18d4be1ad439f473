definitions:
  baseVO:
    type: object
    properties:
      code:
        type: integer
      msg:
        type: string
      data:

        type: string
  UserVO:
    type: object
    properties:
      id:
        type: string
      token:
        type: string
      img:
        type: string
        description: 图片
      name:
        type: string
        description: 用户名

  StoreInfoVO:
    type: object
    properties: 
      status:
        type: string
        description: 店铺状态 
      marketNum:
        type: string
        description: 商场编号
      fid:
        type: string
        description: 地图编号
      tenantName: 
        type: string
        description: 租户名字
      tenantTel:
        type: string
        description: 租户电话
      busyCat:
        type: string
        description: 经营类目
      brand:
        type: string
        description: 品牌
      monthRent: 
        type: integer
        description: 月租
      frontMoney: 
        type: integer
        description: 定金
      contractNum: 
        type: string
        description: 合同编号
      usableArea: 
        type: string
        description: 可用面积
      spec:
        type: string
        description: 规格（长X宽X高）
      totalArea:
        type: string
        description: 总面积
      picName:
        type: string
        description: 负责人
      picTel:
        type: string
        description: 负责人电话
      leaseTime:
        type: string
        description: 日期  

  IndexVO:
    type: object
    description: 首页信息
    properties:
      account:
        type: string
        description: 账号
      position:
        type: string
        description: 职位
      dateflow:
        type: integer
        description: 今日客流
      formatdistri:
        type: array
        items: 
          $ref: "#/definitions/FormatDistriVO"
        
      top3:
        type: array
        description: 店铺转化率 top3
        items:
            $ref: "#/definitions/StoreTransVO"
      todayMsg: 
        type: array
        items:
          type: string
              


  FormatDistriVO:
      type: object
      properties:
        brands:
          type: string
          description: 品牌
        amount:
          type: integer
          description: 数量
        picUrl:
          type: string
            
  
  TrendClientVO:
    type: object
    description: 店铺意向客户
    properties:
      name:
        type: string
      tel:
        type: string
      picname:
        type: string
        description: 负责人姓名
      offer:
        type: integer
        description: 客户报价


  StoreTransVO:
    type: object
    description: 店铺转化率VO
    properties:
      name:
        type: string
        description: 店铺名
      flow:
        type: integer
        description: 流量
      salevolume:
        type: integer
        description: 销售额
      rate:
        type: integer
        description: 转化率

  StoreRankVO:
    type: object
    description: 店铺排行
    properties:
      name:
        type: string
        description: 店铺名
      plateaueffect :
        type: integer
        description: 坪效
      collectpower:
        type: integer
        description: 集客力
      score:
        type: integer
        description: 评分
        
  TodayMsgVO:
    type: object
    description: 实时消息（今日总结）
    properties:
      totalflow:
        type: integer
        description: 总客流
      totalsales :
        type: integer
        description: 总销售额
      longMSg: 
        type: string
        description: 长消息
      shortMsg:
        type: array
        description: 0(到期提醒) 1(客流消息) 2(销售消息) 3(客流销售额) 4(保安) 5(保洁) 
        items:
          type: string 
            


  CleanerVO:
    type: object
    description: 保洁
    properties:
      name:
        type: string
      region:
        type: string
        description: 负责区域
      workTime:
        type: string
        description: 工作时间
      regionList:
        type: string
        description: fid集合  

  GuardVO:
    type: object
    description: 保安
    properties:
      name:
        type: string
      route:
        type: array
        items: 
          type: object
          properties:
            x: 
              type: string
            y: 
              type: string  
              
