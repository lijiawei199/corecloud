definitions:
  storeLeaseTimeVo:
    type: object
    properties:
      expire:
        type: array
        items: 
         type: integer
      unexpire:
        type: array
        items:
          type: integer
  
  storeUseVo:
    type: object
    properties:
      used:
        type: array
        items: 
         type: integer
      notUsed:
        type: array
        items:
          type: integer   
  stroeDateVo:
    type: object
    properties:
      low:
        type: array
        items: 
         type: string
      middle:
        type: array
        items:
          type: string   
      high:
        type: array
        items:
          type: string
          
          
                  
                  
  energyVo:
    type: object
    properties:
      water:
        type: integer
      electric:
        type: integer
        items:
          type: integer   
      gas:
        type: integer
  
  baseVO:
    type: object
    properties:
      code:
        type: integer
      msg:
        type: string
      data:

        type: string
  UserVO:
    type: object
    properties:
